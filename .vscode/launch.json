    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach To npm",
            "type": "node",
            "request": "attach",
            "port": 9229,
            "address": "localhost",
            "restart": true,
            "sourceMaps": false,
            "stopOnEntry": false,
            "localRoot": "${workspaceRoot}",
            "remoteRoot": "${workspaceRoot}",
            "skipFiles": [
                "${workspaceFolder}/node_modules/**/*.js",
            ]
        }
    ]
}